{
  "manifest_version": 2,
  "name": "tt-watcher",
  "version": "1.0.0",
  "default_locale": "zh-CN",
  "description": "Chrome extension to detect tt",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png"
  },

  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "24": "images/icon24.png",
      "32": "images/icon32.png"
    },
    "default_title": "tt-watcher",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ]
}